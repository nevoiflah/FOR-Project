{
  "expo": {
    "name": "F.O.R",
    "slug": "for-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/FOR-logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/FOR-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5DC"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nevoiflah.forproject",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSHealthShareUsageDescription": "FOR needs access to your health data to analyze Sleep and Activity trends.",
        "NSHealthUpdateUsageDescription": "FOR uses health data for personalized recovery coaching."
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.access": []
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/FOR-logo.png",
        "backgroundColor": "#F5F5DC"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/FOR-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "19bbbaf9-a86c-4106-a298-1321f61271df"
      }
    }
  }
}